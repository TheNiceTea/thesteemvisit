<p id="notice"><%= notice %></p>

<h1>Statistics</h1>

<table>
  <thead>
    <tr>
      <th>Clicks</th>
      <th>Ref</th>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statistics.each do |statistic| %>
      <tr>
        <td><%= statistic.clicks %></td>
        <td><%= statistic.ref %></td>
        <td><%= statistic.post_id %></td>
     
      </tr>
    <% end %>
  </tbody>
</table>

<br>
